#include <stdio.h>

int main()
{
    char ch;
    int lower,upper;
    printf("Enter an Alphabet: ");
    scanf("%c", &ch);
    lower= (ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u');
    upper= (ch=='A'||ch=='E'||ch=='I'||ch=='O'||ch=='U');
    if(lower||upper)
    {
        printf("%c is a VOVEL", ch);
    }
    else{
        printf("%c is a CONSONANT", ch);
    }
    return 0;
}
