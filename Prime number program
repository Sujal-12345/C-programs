#include <stdio.h>

int main()
{
    int i,n, counter = 1;
    printf("Enter a number to check for Prime Number: ");
    scanf("%d", &n);
    
    for (i=2; i<n; i++)
    {
        if (n%i==0)
        {
            counter = 0;
            break;
        }
        printf("Loop run for i = %d", i);
    }
    if (counter = 0)
        printf("It is not a Prime Number");
    else
        printf("It is a Prime Number");
    return 0;
}
